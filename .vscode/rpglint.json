{
  "indent": 2,
  "PrototypeCheck": true,
  "NoOCCURS": true,
  "NoSELECTAll": true,
  "UppercaseConstants": true,
  "IncorrectVariableCase": true,
  "StringLiteralDupe": true,
  "NoSQLJoins": true,
  "PrettyComments": true,
  "NoGlobalSubroutines": true,
  "BlankStructNamesCheck": true,
  "IncludeMustBeRelative": false,
  "NoCTDATA": true,
  "CopybookDirective": "copy",
  "ForceOptionalParens": true,
  "NoExecuteImmediate": true,
  "NoExtProgramVariable": false,
  "NoGlobalsInProcedures": true,
  "NoLocalSubroutines": true,
  "NoUnreferenced": false,
  "QualifiedCheck": true,
  "RequireBlankSpecial": true,
  "RequireOtherBlock": true,
  "RequiresParameter": true,
  "RequiresProcedureDescription": true,
  "SQLHostVarCheck": true,
  "UppercaseDirectives": true,
  "NoExternalTo": [
    "QCMD",
    "QP2TERM",
    "QSH",
    "SYSTEM",
    "QCMDEXC"
  ],
  "SpecificCasing": [
    {
      "operation": "varchar",
      "expected": "VarChar"
    },
    {
      "operation": "sqltype",
      "expected": "SqlType"
    },
    {
      "operation": "dcl-f",
      "expected": "Dcl-F"
    }
  ]
}